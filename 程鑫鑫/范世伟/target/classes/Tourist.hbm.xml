<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.fsw.model.entity.Tourist" table="tourist" schema="lxtourism">
        <id name="touristId" column="touristId"/>
        <property name="idCard" column="idCard"/>
        <property name="tel" column="tel"/>
        <property name="realName" column="realName"/>
    </class>
</hibernate-mapping>