<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.fsw.model.entity.Linetype" table="linetype" schema="lxtourism">
        <id name="lineTypeId" column="lineTypeId"/>
        <property name="typeName" column="typeName"/>
        <property name="time" column="time"/>
        <property name="icon" column="icon"/>
        <set name="lineSet" inverse="true" cascade="all">
            <key column="lineTypeId"></key>
            <one-to-many class="com.fsw.model.entity.Line"/>
        </set>
    </class>
</hibernate-mapping>