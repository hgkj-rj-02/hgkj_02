<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hgkj.model.entity.Tourist" table="tourist" schema="travel">
        <id name="touristId" column="touristID"/>
        <property name="tourIdCard" column="tourIDCard"/>
        <property name="tourTel" column="tourTel"/>
        <property name="tourRealName" column="tourRealName"/>
    </class>
</hibernate-mapping>