<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hgkj.model.entity.LineType" table="linetype" schema="travel">
        <id name="lineTypeId" column="lineTypeID"/>
        <property name="lineTypeName" column="lineTypeName"/>
        <property name="lineTypeTime" column="lineTypeTime"/>
        <property name="lineTypeIcon" column="lineTypeIcon"/>
        <set name="lineSet" cascade="all" lazy="false">
            <key column="lineTypeId"/>
            <one-to-many class="com.hgkj.model.entity.Line"/>
        </set>
    </class>
</hibernate-mapping>